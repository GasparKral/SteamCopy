{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./*"],
            "@components/*": ["./components/*"],
            "@hooks/*": ["./hooks/*"],
            "@layouts/*": ["./layouts/*"],
            "@pages/*": ["./pages/*"],
            "@assets/*": ["./assets/*"],
            "@reactC/*": ["./components/react/*"],
            "@astroC/*": ["./components/astro/*"],
            "@stores/*": ["./stores/*"],
            "@types/*": ["./types/*"],
            "@data/*": ["./data/*"]
        }
    }
}
